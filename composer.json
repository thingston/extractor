{
    "name": "thingston/extractor",
    "type": "library",
    "description": "Collection of PHP classes to extract data from HTML pages.",
    "keywords": ["php", "dom", "parser"],
    "homepage": "https://thingston.com",
    "license": "MIT",
    "authors": [
        {"name": "Pedro Ferreira", "email": "pedro@thingston.com"}
    ],
    "require": {
        "php": "^7.1",
        "symfony/dom-crawler": "^5.0",
        "symfony/css-selector": "^5.0",
        "patrickschur/language-detection": "^3.4",
        "guzzlehttp/psr7": "^1.4",
        "jwage/purl": "^0.0.10",
        "neitanod/forceutf8": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Thingston\\": "src/Thingston/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThingstonTest\\": "test/"
        }
    }
}
